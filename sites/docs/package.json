{
	"name": "docs",
	"description": "Docs for Runed",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
		"dev:content": "contentlayer dev",
		"dev:svelte": "vite dev --host",
		"build": "contentlayer build && vite build",
		"build:all": "pnpm -w build",
		"build:content": "contentlayer build",
		"build:content-cachebust": "contentlayer build --clearCache",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"sync": "svelte-kit sync && pnpm build:content"
	},
	"license": "MIT",
	"contributors": [
		{
			"name": "Thomas G. Lopes",
			"url": "https://thomasglopes.com"
		},
		{
			"name": "Hunter Johnston",
			"url": "https://github.com/huntabyte"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/svecosystem/runed.git"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.3",
		"autoprefixer": "^10.4.17",
		"clsx": "^2.1.0",
		"concurrently": "^8.2.2",
		"contentlayer": "^0.3.4",
		"mdsx": "^0.0.6",
		"phosphor-svelte": "^1.4.2",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"rehype-pretty-code": "^0.13.0",
		"remark-gfm": "^4.0.0",
		"shiki": "^1.1.3",
		"svelte": "5.0.0-next.169",
		"svelte-check": "^3.6.4",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.2.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unified": "^11.0.4",
		"unist-builder": "^4.0.0",
		"unist-util-visit": "^5.0.0",
		"vite": "^5.1.3",
		"vitest": "^1.6.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@melt-ui/svelte": "^0.83.0",
		"bits-ui": "^0.21.11",
		"mode-watcher": "^0.3.1",
		"rehype-slug": "^6.0.0",
		"runed": "workspace:^",
		"svelte-sonner": "^0.3.24",
		"vaul-svelte": "^0.3.2"
	}
}
